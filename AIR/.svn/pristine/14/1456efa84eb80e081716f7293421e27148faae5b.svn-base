		private function arrangDiscountsListForSaving(line:ReservationLineHBox):ArrayCollection
		{
			var list:ArrayCollection = new ArrayCollection;
			
			for(var n:int=0; n<line.discountList.length; n++)
			{
				var disc:Discounts = line.discountList.getItemAt(n) as Discounts;
				var obj:ReservationDiscounts = getAlreadySavedDiscounts(line.savedDiscountList, disc.uuid);
					obj.discountId			 = disc.uuid;
					
				if(disc.toBeAppliedForCalculation == true && disc.applicableStatus == true)
				{
						obj.amountType			 = disc.amountType;
						obj.discountAmount		 = disc.amount;
						obj.dsType				 = disc.dsType;
						obj.status 				 = JWHConstants.STATUS_LABLE_ACTIVE;
						
						obj.timeStamp.createdById 		= _loggedInUser.uuid;
						obj.timeStamp.createdByName 	= _loggedInUser.userName;
					
						obj.timeStamp.updatedById 		= _loggedInUser.uuid;
						obj.timeStamp.updatedByName 	= _loggedInUser.userName;
						list.addItem(obj);	
				}
				else
				{
					if(checkAlreadySavedDiscounts(line.savedDiscountList, disc.uuid))
					{
						obj.status = JWHConstants.STATUS_LABLE_INACTIVE;
						list.addItem(obj);	
					}
				}
					
				
			}
				
			
			return list;
		}
		
		private function checkAlreadySavedDiscounts(list:ArrayCollection, inId:String):Boolean
		{
			var isAlreadyIn:Boolean = false;
			
			for(var i:int=0; i<list.length; i++)
        	{
        		var disc:ReservationDiscounts = list.getItemAt(i) as ReservationDiscounts;
        		if(disc.discountId == inId)
        		{
        			isAlreadyIn = true;
        			break;
        		}
        			
        	}
        	return isAlreadyIn;
		}
		
		private function getAlreadySavedDiscounts(list:ArrayCollection, inId:String):ReservationDiscounts
		{
			var disc:ReservationDiscounts = new ReservationDiscounts;
			
			for(var i:int=0; i<list.length; i++)
        	{
        		
        		if(list.getItemAt(i).discountId == inId)
        		{
        			disc = list.getItemAt(i) as ReservationDiscounts;
        			break;
        		}
        			
        	}
        	return disc;
		}
		
